{
  "name": "fabricator",
  "version": "1.3.1",
  "description": "A tool for building website UI toolkits",
  "license": "MIT",
  "homepage": "http://fbrctr.github.io/",
  "scripts": {
    "build": "webpack --define process.env.NODE_ENV='production'",
    "start": "concurrently --kill-others \"npm run server\" \"npm run watch\"",
    "watch": "node watch-template.js",
    "server": "webpack-dev-server --define process.env.NODE_ENV='development'",
    "test": "npm run build"
  },
  "keywords": [
    "toolkit",
    "styleguide",
    "framework",
    "scaffold",
    "patterns",
    "library"
  ],
  "author": "Luke Askew",
  "licenses": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fbrctr/fabricator.git"
  },
  "engines": {
    "node": ">=4.0",
    "npm": ">=2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.5.0",
    "browser-sync": "^2.13.0",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "del": "^2.2.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "fabricator-assemble": "^1.2.0",
    "image-webpack-loader": "^3.3.1",
    "imagemin-webpack-plugin": "^1.5.0-beta.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^4.5.3",
    "on-build-webpack": "^0.1.0",
    "postcss-loader": "^2.0.6",
    "pre-build-webpack": "^0.1.0",
    "run-sequence": "^1.2.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "watch": "^1.0.2",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "foundation-sites": "^6.4.2"
